{
  "input": [
    {
      "type": "maxmindGeoLite2CountryCSV",
      "action": "add",
      "args": {
        "country": "./geolite2/GeoLite2-Country-Locations-en.csv",
        "ipv4": "./geolite2/GeoLite2-Country-Blocks-IPv4.csv",
        "ipv6": "./geolite2/GeoLite2-Country-Blocks-IPv6.csv",
        "wantedList": ["ru", "by", "kz"]
      }
    },
    {
      "type": "text",
      "action": "add", 
      "args": {
        "name": "ru",
        "uri": "https://raw.githubusercontent.com/hydraponique/roscomvpn-geoip/refs/heads/master/addcustom/ru"
      }
    },
    {
      "type": "text",
      "action": "add", 
      "args": {
        "name": "by",
        "uri": "https://raw.githubusercontent.com/hydraponique/roscomvpn-geoip/refs/heads/master/addcustom/by"
      }
    },
    {
      "type": "text",
      "action": "add", 
      "args": {
        "name": "kz",
        "uri": "https://raw.githubusercontent.com/hydraponique/roscomvpn-geoip/refs/heads/master/addcustom/kz"
      }
    },
    {
      "type": "private",
      "action": "add"
    },
    {
      "type": "test",
      "action": "add"
    }
  ],
  "output": [
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputName": "geoip.dat"
      }
    },
    {
      "type": "text",
      "action": "output"
    }
  ]
}
